: # -*-Perl-*-
eval 'exec perl -w "$0" ${1+"$@"}'
   if 0;

use Asim;
use strict;

##
## Generate cfg files for Alpha tests
##

my $gcfg = Asim::GenCFG->new();

my @bmarks = ("hello");
	      
foreach my $bmark (@bmarks) {
    $gcfg->add(name => $bmark,
               tree => [ "user_mode" ],
               setup => "tools/scripts/bmark/setup-bmark $bmark --group tests/$bmark --isa alpha",
               feeder => "m5",
               feedflags => "m5_configs/se.py --hasim-sim --cmd program/${bmark}.alpha.bin",
              );
}

$gcfg->action(@ARGV);
